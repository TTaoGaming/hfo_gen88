{
  "name": "hfo-gen88-root",
  "version": "88.0.0",
  "description": "HFO Gen 88 Root Tooling (Immune System)",
  "type": "module",
  "scripts": {
    "scream": "npx tsx hot_obsidian_sandbox/bronze/2_areas/hfo_ports/P4_RED_REGNANT/RED_REGNANT.ts",
    "stryker": "stryker run hot_obsidian_sandbox/bronze/2_areas/infra/configs/stryker.root.config.mjs",
    "stryker:silver": "stryker run hot_obsidian_sandbox/bronze/2_areas/infra/configs/stryker.silver.config.mjs",
    "test": "vitest run -c hot_obsidian_sandbox/bronze/2_areas/infra/configs/vitest.root.config.ts",
    "test:silver": "vitest run -c hot_obsidian_sandbox/bronze/2_areas/infra/configs/vitest.silver.config.ts",
    "test:silver:watch": "vitest -c hot_obsidian_sandbox/bronze/2_areas/infra/configs/vitest.silver.config.ts",
    "silver:health": "vitest run --config hot_obsidian_sandbox/silver/1_projects/health-check/vitest.silver.config.ts",
    "silver:health:receipt": "npx tsx hot_obsidian_sandbox/silver/1_projects/health-check/run-health-check.ts",
    "silver:gate": "npm run scream",
    "silver:nightly": "npx tsx hot_obsidian_sandbox/silver/1_projects/health-check/nightly-regression.ts",
    "precommit": "npm run silver:gate",
    "prepare": "husky"
  },
  "devDependencies": {
    "@stryker-mutator/core": "^8.0.0",
    "@stryker-mutator/typescript-checker": "^8.0.0",
    "@stryker-mutator/vitest-runner": "^8.0.0",
    "@types/js-yaml": "^4.0.9",
    "duckdb": "^1.4.3",
    "fast-check": "^4.5.3",
    "husky": "^9.1.7",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^1.2.1",
    "zod": "^3.22.4"
  },
  "dependencies": {
    "@dimforge/rapier2d-compat": "^0.19.3",
    "@mastra/core": "^0.24.9",
    "@mastra/libsql": "^0.16.4",
    "@mastra/memory": "^0.15.13",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.208.0",
    "@opentelemetry/instrumentation": "^0.208.0",
    "@opentelemetry/sdk-trace-base": "^2.2.0",
    "@opentelemetry/sdk-trace-node": "^2.2.0",
    "@temporalio/activity": "^1.14.0",
    "@temporalio/client": "^1.14.0",
    "@temporalio/worker": "^1.14.0",
    "@temporalio/workflow": "^1.14.0",
    "js-yaml": "^4.1.1"
  }
}
